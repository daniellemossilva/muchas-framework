{
  "name": "muchas-framework",
  "version": "0.0.1",
  "description": "Muchas Framework - NodeJS Microservice Framework",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "nodemon",
    "build:dev": "ts-node ./src/index.ts",
    "dev": "tsc -w --skipLibCheck",
    "lint": "eslint .",
    "build:prd": "tsc --skipLibCheck",
    "start:prd": "node dist/",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "testWithCoverage": "nyc -r lcov -e .ts -x \"*.test.ts\" mocha -r ts-node/register tests/**/*.test.ts && nyc report && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/muchas/muchas-framework.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "microservice",
    "framework"
  ],
  "author": "Henrique Marques Fernandes <henrique@marquesfernandes.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/muchas/muchas-framework/issues"
  },
  "homepage": "https://gitlab.com/muchas/muchas-framework#README",
  "devDependencies": {
    "@types/node": "^11.13.4",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/mocha": "^5.2.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "mocha": "^6.1.2",
    "nyc": "^13.3.0",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
    "@types/elasticsearch": "^5.0.32",
    "@types/express": "^4.16.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.3.24",
    "@types/uniqid": "^4.1.3",
    "elasticsearch": "^15.4.1",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.22",
    "uniqid": "^5.0.3",
    "winston": "^3.2.1",
    "winston-elasticsearch": "^0.7.9"
  }
}
